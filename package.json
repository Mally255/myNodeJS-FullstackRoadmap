{
  "name": "fullstack-roadmap",
  "version": "1.0.0",
  "description": "A roadmap to what is needed for being a fullstack developer",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage "
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js",
      "controllers/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "author": "Malcolm R. Kente",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^25.4.0",
    "supertest": "^4.0.2"
  }
}
